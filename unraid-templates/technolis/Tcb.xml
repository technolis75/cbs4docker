<?xml version="1.0" encoding="utf-8"?>
<Container>
	<Name>Tcb</Name>
	<Description>Technolis Cloud Backup - backup your files locally and/or into a cloud</Description>
	<Registry>https://hub.docker.com/r/technolis75/cbs4pub/</Registry>
	<Repository>technolis75/cbs4pub</Repository>
	<GitHub>https://github.com/technolis75/cbs4docker</GitHub>
	<Privileged>false</Privileged>
	<Environment/>
	<Project>http://www.technolis.fr</Project>
	<Beta>True</Beta>
	<Category>Backup: Cloud:</Category>
	<Date>2015-10-28</Date> 
	<Networking>
		<Mode>bridge</Mode>
		<Publish>
			<Port>
				<HostPort>81</HostPort>
				<ContainerPort>80</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
			<Port>
				<HostPort>443</HostPort>
				<ContainerPort>443</ContainerPort>
				<Protocol>tcp</Protocol>
			</Port>
		</Publish>
	</Networking>
	<Data>
		<Volume>
			<HostDir>/mnt/user/backup/tcb/logs</HostDir>
			<ContainerDir>/usr/local/cbs/logs</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
		<Volume>
			<HostDir>/mnt/user/backup/tcb/config</HostDir>
			<ContainerDir>/usr/local/cbs/conf</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
		<Volume>
			<HostDir>/mnt/user/backup/tcb/users</HostDir>
			<ContainerDir>/usr/local/cbs/user</ContainerDir>
			<Mode>rw</Mode>
		</Volume>
		
	</Data>
	<Version>latest</Version>
	<WebUI>http://[IP]:[PORT:80]/</WebUI>
	<Banner>http://www.technolis.fr/images/logo.png</Banner>
	<Icon>http://www.technolis.fr/images/S-35-DE5_01.gif</Icon>
	<ExtraParams/>
</Container>
