<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>TCB</Name>
  <Description>Technolis Cloud Backup - backup your files locally and/or into a cloud</Description>
  <Registry>https://registry.hub.docker.com/u/technolis75/cbs4pub/</Registry>
  <Repository>technolis75/cbs4docker</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>81</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/backup/tcb/logs</HostDir>
      <ContainerDir>/usr/local/cbs/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/backup/tcb/users</HostDir>
      <ContainerDir>/usr/local/cbs/user</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>05616fd2</Version>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Banner>http://www.technolis.fr/images/logo.png</Banner>
  <Icon>http://www.technolis.fr/images/S-35-DE5_01.gif</Icon>
  <ExtraParams></ExtraParams>
</Container>
